{
  "manifest_version": 3,
  "name": "Отключение текста для VK Style",
  "version": "1.0",
  "description": "В режиме плеера убирает текст",
  "permissions": ["activeTab"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.vk.com/*"],
      "js": ["content.js"]
    }
  ]
}